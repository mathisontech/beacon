# Development environment variables for Beacon GPS app
REACT_APP_ENV=development

# Suppress React DevTools suggestion in console
REACT_DEVTOOLS_LOGGER=false

# Enable debug modes (set to true if you want verbose logging)
DEBUG_WEATHER=false
DEBUG_GPS=false

# Development features
VITE_DEV_MODE=true

# Performance optimizations
VITE_ENABLE_HIGH_ACCURACY_GPS=false
VITE_REDUCED_POLLING=true
VITE_MAP_PERFORMANCE_MODE=true