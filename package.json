{
  "name": "beacon-emergency-gps",
  "version": "1.0.0",
  "description": "Beacon - Emergency GPS Navigation System with OpenStreetMap integration",
  "main": "frontend/src/main.jsx",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
